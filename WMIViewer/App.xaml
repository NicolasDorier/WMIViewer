<Application x:Class="WMIViewer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WMIViewer"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/AODesign;component/Styles/All.xaml" />
            </ResourceDictionary.MergedDictionaries>
            
            <local:VisibilityConverter x:Key="FalseToHidden" Collapsed="Hidden"></local:VisibilityConverter>
        </ResourceDictionary>
    </Application.Resources>
</Application>
